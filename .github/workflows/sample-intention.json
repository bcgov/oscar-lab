{
  "event": {
    "provider": "sample-github-action",
    "reason": "Job triggered",
    "transient": true,
    "url": "JOB_URL"
  },
  "actions": [
    {
      "action": "package-configure",
      "id": "configure",
      "provision": ["token/self"],
      "service": {
        "name": "oscar-lab",
        "project": "oscar",
        "environment": "development"
      }
    }
  ],
  "user": {
    "name": "github@internal"
  }
}